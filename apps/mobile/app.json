{
  "expo": {
    "name": "モグネイター",
    "slug": "mognator",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mognator.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "近くの飲食店を検索するために位置情報を使用します。"
      }
    },
    "android": {
      "package": "com.mognator.mobile",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {}
  }
}
